@model DefaultConnection.ww_Re
@{
    ViewBag.Title = "ModifiRes";
}

<h2>>>修改物证信息</h2>
<form class="form-horizontal" role="form" action="/Case/DoModifiRes" method="POST" >
    <div class="form-group hidden ">
        <label  class="col-sm-2 control-label">物证ID</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  name="ResID" value="@Model.ResID"
                >
        </div>
    </div>
     <div class="form-group  ">
        <label  class="col-sm-2 control-label">物证名称</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  name="ResNmae" value="@Model.ResNmae"
          placeholder="物证名称"       >
        </div>
    </div>
  
   <div class="form-group">
      <label  class="col-sm-2 control-label">简要物证信息</label>
      <div class="col-sm-10">
         <input type="text" class="form-control" name="ResMess"  value="@Model.ResMess"
            placeholder="请输入简要物证信息">
      </div>
   </div>
   
   <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
          <button type="submit" class="btn btn-default">提交</button>
           <button type="submit" class="btn btn-default">取消</button>
      </div>
   </div>
</form>
